<?xml version="1.0" encoding="UTF-8"?>
<gdml xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="http://service-spi.web.cern.ch/service-spi/app/releases/GDML/schema/gdml.xsd">

  <define>
    <constant name="PI" value="1.0*pi"/>
    <constant name="TWOPI" value="2.0*pi"/>
    <position name="center" x="0" y="0" z="0" unit="mm"/>
    <rotation name="rot_phi_0" x="0" y="0" z="0.0" unit="deg"/>
    <rotation name="rot_phi_1" x="0" y="0" z="72.0" unit="deg"/>
    <rotation name="rot_phi_2" x="0" y="0" z="144.0" unit="deg"/>
    <rotation name="rot_phi_3" x="0" y="0" z="216.0" unit="deg"/>
    <rotation name="rot_phi_4" x="0" y="0" z="288.0" unit="deg"/>
  </define>

  <materials>
    <!-- Element definitions -->
    <element name="Hydrogen" formula="H" Z="1">
      <atom value="1.00794"/>
    </element>
    <element name="Carbon" formula="C" Z="6">
      <atom value="12.0107"/>
    </element>
    <element name="Nitrogen" formula="N" Z="7">
      <atom value="14.0067"/>
    </element>
    <element name="Oxygen" formula="O" Z="8">
      <atom value="15.9994"/>
    </element>
    <element name="Iron_el" formula="Fe" Z="26">
      <atom value="55.845"/>
    </element>
    <element name="Chromium" formula="Cr" Z="24">
      <atom value="51.9961"/>
    </element>
    <element name="Nickel" formula="Ni" Z="28">
      <atom value="58.6934"/>
    </element>
    <element name="Lead_el" formula="Pb" Z="82">
      <atom value="207.2"/>
    </element>
    <element name="Tungsten_el" formula="W" Z="74">
      <atom value="183.84"/>
    </element>

    <!-- Air -->
    <material name="G4_AIR" formula="Air">
      <D value="0.00120479" unit="g/cm3"/>
      <fraction n="0.7" ref="Nitrogen"/>
      <fraction n="0.3" ref="Oxygen"/>
    </material>

    <!-- Stainless Steel (typical 304) -->
    <material name="G4_STAINLESS-STEEL" formula="Steel">
      <D value="7.93" unit="g/cm3"/>
      <fraction n="0.70" ref="Iron_el"/>
      <fraction n="0.19" ref="Chromium"/>
      <fraction n="0.10" ref="Nickel"/>
      <fraction n="0.01" ref="Carbon"/>
    </material>

    <!-- Iron -->
    <material name="G4_Fe" formula="Fe">
      <D value="7.874" unit="g/cm3"/>
      <fraction n="1.0" ref="Iron_el"/>
    </material>

    <!-- Lead -->
    <material name="G4_Pb" formula="Pb">
      <D value="11.35" unit="g/cm3"/>
      <fraction n="1.0" ref="Lead_el"/>
    </material>

    <!-- Tungsten -->
    <material name="G4_W" formula="W">
      <D value="19.3" unit="g/cm3"/>
      <fraction n="1.0" ref="Tungsten_el"/>
    </material>

    <!-- Plastic Scintillator (polyvinyltoluene based) -->
    <material name="G4_PLASTIC_SC_VINYLTOLUENE" formula="C9H10">
      <D value="1.032" unit="g/cm3"/>
      <fraction n="0.915" ref="Carbon"/>
      <fraction n="0.085" ref="Hydrogen"/>
    </material>

    <!-- Polystyrene -->
    <material name="G4_POLYSTYRENE" formula="C8H8">
      <D value="1.06" unit="g/cm3"/>
      <fraction n="0.923" ref="Carbon"/>
      <fraction n="0.077" ref="Hydrogen"/>
    </material>

    <!-- Silicon -->
    <element name="Silicon_el" formula="Si" Z="14">
      <atom value="28.0855"/>
    </element>
    <material name="G4_Si" formula="Si">
      <D value="2.33" unit="g/cm3"/>
      <fraction n="1.0" ref="Silicon_el"/>
    </material>

    <!-- Crystal calorimeter materials -->
    <element name="Cesium" formula="Cs" Z="55">
      <atom value="132.905"/>
    </element>
    <element name="Iodine" formula="I" Z="53">
      <atom value="126.904"/>
    </element>
    <material name="G4_CESIUM_IODIDE" formula="CsI">
      <D value="4.51" unit="g/cm3"/>
      <fraction n="0.512" ref="Cesium"/>
      <fraction n="0.488" ref="Iodine"/>
    </material>

    <element name="Bismuth" formula="Bi" Z="83">
      <atom value="208.98"/>
    </element>
    <element name="Germanium" formula="Ge" Z="32">
      <atom value="72.63"/>
    </element>
    <material name="G4_BGO" formula="Bi4Ge3O12">
      <D value="7.13" unit="g/cm3"/>
      <fraction n="0.671" ref="Bismuth"/>
      <fraction n="0.175" ref="Germanium"/>
      <fraction n="0.154" ref="Oxygen"/>
    </material>

    <material name="G4_PbWO4" formula="PbWO4">
      <D value="8.28" unit="g/cm3"/>
      <fraction n="0.455" ref="Lead_el"/>
      <fraction n="0.404" ref="Tungsten_el"/>
      <fraction n="0.141" ref="Oxygen"/>
    </material>

  </materials>

  <solids>
    <box name="world_solid" x="50000.0" y="50000.0" z="50000.0" lunit="mm"/>
    <tube name="abs_eta0_layer0" rmin="1500.0" rmax="1501.0" z="3193.9" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="act_eta0_layer0" rmin="1501.0" rmax="1643.4" z="3193.9" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="abs_eta1_layer0" rmin="1500.0" rmax="1501.0" z="781.6" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="act_eta1_layer0" rmin="1501.0" rmax="1643.4" z="781.6" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="abs_eta2_layer0" rmin="1500.0" rmax="1501.0" z="781.6" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="act_eta2_layer0" rmin="1501.0" rmax="1643.4" z="781.6" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="abs_eta3_layer0" rmin="1500.0" rmax="1501.0" z="3193.9" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="act_eta3_layer0" rmin="1501.0" rmax="1643.4" z="3193.9" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="abs_eta4_layer0" rmin="1500.0" rmax="1501.0" z="5000.0" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="act_eta4_layer0" rmin="1501.0" rmax="1643.4" z="5000.0" startphi="0" deltaphi="72.0" aunit="deg" lunit="mm"/>
    <tube name="calor_envelope" rmin="1500.0" rmax="1643.4" z="10000.0" deltaphi="TWOPI" aunit="rad" lunit="mm"/>
  </solids>

  <structure>
    <volume name="absorber_log_eta0_layer0">
      <materialref ref="G4_AIR"/>
      <solidref ref="abs_eta0_layer0"/>
    </volume>
    <volume name="active_log_eta0_layer0">
      <materialref ref="G4_PbWO4"/>
      <solidref ref="act_eta0_layer0"/>
      <auxiliary auxtype="SensDet" auxvalue="CalorimeterSD"/>
    </volume>
    <volume name="absorber_log_eta1_layer0">
      <materialref ref="G4_AIR"/>
      <solidref ref="abs_eta1_layer0"/>
    </volume>
    <volume name="active_log_eta1_layer0">
      <materialref ref="G4_PbWO4"/>
      <solidref ref="act_eta1_layer0"/>
      <auxiliary auxtype="SensDet" auxvalue="CalorimeterSD"/>
    </volume>
    <volume name="absorber_log_eta2_layer0">
      <materialref ref="G4_AIR"/>
      <solidref ref="abs_eta2_layer0"/>
    </volume>
    <volume name="active_log_eta2_layer0">
      <materialref ref="G4_PbWO4"/>
      <solidref ref="act_eta2_layer0"/>
      <auxiliary auxtype="SensDet" auxvalue="CalorimeterSD"/>
    </volume>
    <volume name="absorber_log_eta3_layer0">
      <materialref ref="G4_AIR"/>
      <solidref ref="abs_eta3_layer0"/>
    </volume>
    <volume name="active_log_eta3_layer0">
      <materialref ref="G4_PbWO4"/>
      <solidref ref="act_eta3_layer0"/>
      <auxiliary auxtype="SensDet" auxvalue="CalorimeterSD"/>
    </volume>
    <volume name="absorber_log_eta4_layer0">
      <materialref ref="G4_AIR"/>
      <solidref ref="abs_eta4_layer0"/>
    </volume>
    <volume name="active_log_eta4_layer0">
      <materialref ref="G4_PbWO4"/>
      <solidref ref="act_eta4_layer0"/>
      <auxiliary auxtype="SensDet" auxvalue="CalorimeterSD"/>
    </volume>
    <volume name="calor_log">
      <materialref ref="G4_AIR"/>
      <solidref ref="calor_envelope"/>
      <physvol name="absorber_0">
        <volumeref ref="absorber_log_eta0_layer0"/>
        <rotationref ref="rot_phi_0"/>
      </physvol>
      <physvol name="active_0">
        <volumeref ref="active_log_eta0_layer0"/>
        <rotationref ref="rot_phi_0"/>
      </physvol>
      <physvol name="absorber_1">
        <volumeref ref="absorber_log_eta1_layer0"/>
        <rotationref ref="rot_phi_0"/>
      </physvol>
      <physvol name="active_1">
