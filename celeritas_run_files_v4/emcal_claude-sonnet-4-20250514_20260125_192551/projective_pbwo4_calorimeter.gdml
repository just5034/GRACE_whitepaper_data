      </physvol>
      <physvol name="active_22">
        <volumeref ref="active_log_eta2_layer0"/>
        <rotationref ref="rot_phi_4"/>
      </physvol>
      <physvol name="absorber_23">
        <volumeref ref="absorber_log_eta3_layer0"/>
