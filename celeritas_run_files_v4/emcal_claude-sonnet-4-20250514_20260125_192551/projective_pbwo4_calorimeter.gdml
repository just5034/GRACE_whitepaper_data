      </physvol>
      <physvol name="active_12">
        <volumeref ref="active_log_eta2_layer0"/>
        <rotationref ref="rot_phi_2"/>
      </physvol>
      <physvol name="absorber_13">
        <volumeref ref="absorber_log_eta3_layer0"/>
        <rotationref ref="rot_phi_2"/>
      </physvol>
      <physvol name="active_13">
        <volumeref ref="active_log_eta3_layer0"/>
        <rotationref ref="rot_phi_2"/>
      </physvol>
      <physvol name="absorber_14">
        <volumeref ref="absorber_log_eta4_layer0"/>
