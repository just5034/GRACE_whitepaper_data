      </physvol>
      <physvol name="active_4">
        <volumeref ref="active_log_eta4_layer0"/>
        <rotationref ref="rot_phi_0"/>
      </physvol>
      <physvol name="absorber_5">
        <volumeref ref="absorber_log_eta0_layer0"/>
        <rotationref ref="rot_phi_1"/>
      </physvol>
      <physvol name="active_5">
        <volumeref ref="active_log_eta0_layer0"/>
        <rotationref ref="rot_phi_1"/>
      </physvol>
      <physvol name="absorber_6">
        <volumeref ref="absorber_log_eta1_layer0"/>
        <rotationref ref="rot_phi_1"/>
      </physvol>
      <physvol name="active_6">
        <volumeref ref="active_log_eta1_layer0"/>
        <rotationref ref="rot_phi_1"/>
      </physvol>
      <physvol name="absorber_7">
        <volumeref ref="absorber_log_eta2_layer0"/>
        <rotationref ref="rot_phi_1"/>
      </physvol>
      <physvol name="active_7">
        <volumeref ref="active_log_eta2_layer0"/>
