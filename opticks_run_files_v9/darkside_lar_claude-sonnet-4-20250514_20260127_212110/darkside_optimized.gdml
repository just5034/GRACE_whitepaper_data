    <tube name="vessel_outer" rmin="0" rmax="1800.0" z="3600.0" deltaphi="TWOPI" startphi="0" aunit="rad" lunit="mm"/>
    <tube name="vessel_inner" rmin="0" rmax="1750.0" z="3500.0" deltaphi="TWOPI" startphi="0" aunit="rad" lunit="mm"/>
    <tube name="sensor_solid" rmin="0" rmax="100.0" z="10.0" deltaphi="TWOPI" startphi="0" aunit="rad" lunit="mm"/>

